{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@static/*": ["src/public/*"],
      "@api/*": ["src/modules/api/*"],
      "@common_utils/*": ["src/modules/common_utils/*"],
      "@survey/*": ["src/survey/*"]
    }
  }
}
